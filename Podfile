# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Swiftui+BitcoinSampleApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Swiftui+BitcoinSampleApp
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Charts'
  pod 'Alamofire', '~> 5.0.0-beta.7'

  target 'Swiftui+BitcoinSampleAppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SwiftyMocky'
    pod 'RxBlocking'
    pod 'RxTest'
  end

  target 'Swiftui+BitcoinSampleAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
